{
  "name": "uppy-server",
  "version": "0.0.1",
  "description": "Almost as cute as a Puppy :dog:",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "deploy": "source env.sh && ${FREYCMD:-frey} setup --project-dir ./infra",
    "deploy:localfrey": "env FREYCMD=\"babel-node ${HOME}/code/frey/src/cli.js\" npm run deploy",
    "destroy": "source env.sh && ${FREYCMD:-frey} destroy --project-dir ./infra",
    "install": "source env.sh && ${FREYCMD:-frey} install --project-dir ./infra",
    "launch": "source env.sh && ${FREYCMD:-frey} infra --project-dir ./infra",
    "plan": "source env.sh && ${FREYCMD:-frey} plan --bail --project-dir ./infra",
    "prepare": "source env.sh && ${FREYCMD:-frey} prepare --bail --project-dir ./infra",
    "refresh": "source env.sh && ${FREYCMD:-frey} refresh --bail --project-dir ./infra",
    "remote": "source env.sh && ${FREYCMD:-frey} remote --project-dir ./infra",
    "restart": "source env.sh && ${FREYCMD:-frey} restart --bail --project-dir ./infra",
    "show": "source env.sh && ${FREYCMD:-frey} show --bail --project-dir ./infra"
  },
  "author": "Transloadit.com",
  "license": "ISC",
  "homepage": "https://github.com/transloadit/uppy-server#readme",
  "devDependencies": {
    "eslint": "1.10.3",
    "eslint-config-standard": "4.4.0",
    "eslint-plugin-standard": "1.3.1",
    "frey": "kvz/frey#8017ffb972632392da2b34f2ba40ac98bb7cc3f0",
    "nodemon": "^1.8.1"
  },
  "dependencies": {
    "dropbox": "^0.10.3",
    "google-auth-library": "^0.9.7",
    "googleapis": "^2.1.7",
    "grant-koa": "^3.5.3",
    "instagram-node": "^0.5.8",
    "koa": "^1.1.2",
    "koa-cors": "0.0.16",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.3.0",
    "koa-session": "^3.3.1",
    "purest": "^2.0.0"
  }
}
